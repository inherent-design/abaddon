    "init": {
      "name": "init",
      "description": "Initialize a new project with default configuration",
      "usage": "abaddon init [project-name] [--type=TYPE] [--force]",
      "examples": "abaddon init my-project --type=web\nabaddon init --force"
    },
    "build": {
      "name": "build", 
      "description": "Build project assets and generate distribution files",
      "usage": "abaddon build [--target=TARGET] [--debug]",
      "examples": "abaddon build --target=production\nabaddon build --debug"
    },
    "test": {
      "name": "test",
      "description": "Run project test suite with coverage reporting",
      "usage": "abaddon test [--coverage] [--verbose] [PATTERN]",
      "examples": "abaddon test --coverage\nabaddon test core"
    },
    "clean": {
      "name": "clean",
      "description": "Clean build artifacts and temporary files",
      "usage": "abladdon clean [--all] [--cache]",
      "examples": "abaddon clean --all\nabaddon clean --cache"
    },
    "status": {
      "name": "status",
      "description": "Show project status and configuration information",
      "usage": "abaddon status [--verbose] [--tools]",
      "examples": "abaddon status --verbose\nabaddon status --tools"
    }
